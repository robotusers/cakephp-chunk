{
	"name": "robotusers/cakephp-chunk",
	"description": "Robotusers CakePHP Chunk plugin",
	"homepage": "https://github.com/robotusers/cakephp-chunk",
	"type": "cakephp-plugin",
	"license": "MIT",
	"require": {
		"cakephp/orm": "~5.0"
	},
	"require-dev": {
		"cakephp/cakephp": "~5.0",
		"cakephp/cakephp-codesniffer": "^5.0",
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Robotusers\\Chunk\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"Robotusers\\Chunk\\Test\\": "tests"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
